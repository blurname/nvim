{
  "hover.target": "float",
  "hover.floatConfig": {
    "border": false,
    "highlight": "CocFloating",
    "borderhighlight": "CocFloating"
  },
  "diagnostic.floatConfig": {
    "border": false,
    "highlight": "CocFloating",
    "borderhighlight": "CocFloating"
  },
  "suggest.floatConfig": {
    "border": false,
    "highlight": "CocFloating",
    "borderhighlight": "CocFloating",
    "winblend": 8
  },
  "semanticTokens.enable": true,
  "suggest.noselect": false,
  "suggest.enablePreselect": true,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.autoTrigger": "always",
  "suggest.timeout": 500,
  "suggest.virtualText": true,
  "suggest.completionItemKindLabels": {
    "keyword": "",
    "variable": "",
    "value": "",
    "operator": "Ψ",
    "constructor": "",
    "function": "ƒ",
    "reference": "渚",
    "constant": "",
    "method": "",
    "struct": "פּ",
    "class": "",
    "interface": "",
    "text": "",
    "enum": "",
    "enumMember": "",
    "module": "",
    "color": "",
    "property": "",
    "field": "料",
    "unit": "",
    "event": "鬒",
    "file": "",
    "folder": "",
    "snippet": "",
    "typeParameter": "",
    "default": ""
  },
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "html.autoClosingTags": true,
  "diagnostic.enableSign": true,
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualTextPrefix": " ❯❯ ",
  "diagnostic.virtualText": false,
  "explorer.file.showHiddenFiles": true,
  "explorer.file.reveal.whenOpen": true,
  "explorer.keyMappings.global": {
    "k": "nodePrev",
    "j": "nodeNext",
    "f": "search",
    "F": "searchRecursive",
    "h": "collapse",
    "l": "expand",
    "o": "open",
    "<c-v>": "open:vsplit.intelligent",
		"<c-x>": "open:split.intelligent",
    "a": "addFile",
    "p": "gotoParent",
    "yp": "copyFilepath",
    "yn": "copyFilename",
    "yy": "copyFile",
    "R": "refresh",
    "rn": "rename",
    "dF": "deleteForever",
    "<tab>": "actionMenu",
    "P": "pasteFile"
  },
  "explorer.icon.source": "builtin",
  "explorer.icon.enableNerdfont": true,
  "typescript.suggest.enabled": true,
  "typescript.suggestionActions.enabled": true,
  "javascript.suggest.enabled": true,
  "javascript.suggestionActions.enabled": true,
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell"]
    },
    "nix": {
      "command": "nil",
      "filetypes": ["nix"]
    },
    "elvish": {
      "command": "elvish",
      "args": ["-lsp"],
      "filetypes": ["elvish"]
    },
    "rescript": {
      "enable": true,
      "module": "~/.local/share/nvim/site/pack/packer/start/vim-rescript/server/out/server.js",
      "args": ["--node-ipc"],
      "filetypes": ["rescript"],
      "rootPatterns": ["bsconfig.json"]
    }
  },
  "snippets.ultisnips.enable": false,
  "snippets.snipmate.enable": false,
  "snippets.loadFromExtensions": false,
  "snippets.textmateSnippetsRoots": ["/home/bl/.config/nvim/snippets"],
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "typescript.autoClosingTags": true,
  "eslint.run": "onSave",
  "eslint.autoFixOnSave": true,
  "Lua.telemetry.enable": false,
  "sumneko-lua.enableNvimLuaDev": true,
  "sumneko-lua.enable": true,
  "sumneko-lua.prompt": true,
  "sumneko-lua.checkUpdate": false,
  "git.addGBlameToVirtualText": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.preferences.useAliasesForRenames": true,
  "typescript.preferences.jsxAttributeCompletionStyle": "none", 
  "coc.preferences.useQuickfixForLocations": true,
  "list.source.files.defaultOptions": ["--number-select", "--no-quit"],
  "list.source.grep.args": ["--smart-case"]
}

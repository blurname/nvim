{
  "hover.target": "float",
  "hover.floatConfig": {
    "border": true,
    "highlight": "CocFloating",
    "borderhighlight": "CocFloating"
  },
  "diagnostic.floatConfig": {
    "border": true,
    "highlight": "CocFloating",
    "borderhighlight": "CocFloating"
  },
  "suggest.floatConfig": {
    "border": true,
    "highlight": "CocFloating",
    "borderhighlight": "CocFloating",
    "winblend": 8
  },
  "suggest.noselect": true,
  "suggest.enablePreselect": true,
  "suggest.triggerAfterInsertEnter": false,
  "suggest.autoTrigger": "always",
  "suggest.timeout": 1000,
  "suggest.enablePreview": true,
  "suggest.floatEnable": true,
  "suggest.completionItemKindLabels": {
    "keyword": "",
    "variable": "",
    "value": "",
    "operator": "Ψ",
    "constructor": "",
    "function": "ƒ",
    "reference": "渚",
    "constant": "",
    "method": "",
    "struct": "פּ",
    "class": "",
    "interface": "",
    "text": "",
    "enum": "",
    "enumMember": "",
    "module": "",
    "color": "",
    "property": "",
    "field": "料",
    "unit": "",
    "event": "鬒",
    "file": "",
    "folder": "",
    "snippet": "",
    "typeParameter": "",
    "default": ""
  },
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "html.autoClosingTags": true,
  "diagnostic.enableSign": true,
  "diagnostic.highlightOffset": 1000,
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualTextPrefix": " ❯❯ ",
  "diagnostic.virtualText": false,
  "explorer.file.showHiddenFiles": true,
  "explorer.keyMappings.global": {
    "k": "nodePrev",
    "j": "nodeNext",
    "f": "search",
    "F": "searchRecursive",
    "h": "collapse",
    "l": "expand",
    "o": "open",
    "<c-v>": "open:vsplit.intelligent",
		"<c-x>": "open:split.intelligent",
    "a": "addFile",
    "p": "gotoParent",
    "yp": "copyFilepath",
    "yn": "copyFilename",
    "yy": "copyFile",
    "R": "refresh",
    "rn": "rename",
    "dF": "deleteForever",
    "<tab>": "actionMenu",
    "P": "pasteFile"
  },
  "explorer.icon.source": "builtin",
  "explorer.icon.enableNerdfont": true,
  "typescript.suggest.enabled": true,
  "typescript.suggestionActions.enabled": true,
  "javascript.suggest.enabled": true,
  "javascript.suggestionActions.enabled": true,
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell"]
    },
    "nix": {
      "command": "rnix-lsp",
      "filetypes": ["nix"]
    },
		"lua":{
			"command": "lua-language-server",
			"filetypes": ["lua"]
		}
  },
  "snippets.enableStatusItem": false,
  "snippets.ultisnips.enable": false,
  "snippets.snipmate.enable": false,
  "snippets.loadFromExtensions": false,
  "snippets.textmateSnippetsRoots": ["/home/bl/.config/nvim/snippets"],
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
	"eslint.alwaysShowStatus": true,
	"eslint.autoFixOnSave": true, 
	"eslint.format.enable": true
}
